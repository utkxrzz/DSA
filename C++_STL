#include<bits/stdc++.h>

using namespace std;

int main() {
    vector <int> v;
    for(int i=0; i<10; i++){
        v.push_back(i);
    }
    
    cout<<"The elements in the vector are: ";
    for(int i=0; i<10; i++){
        cout<<v[i]<<" ";
    }
    cout<<endl;
    
     cout<<"The first element of the vector is: "<<v.front()<<endl;
     cout<<"The last element of the vector is: "<<v.back()<<endl;
     cout<<"The size of the vector is: "<<v.size()<<endl;
     cout<<"Deleting the last element: "<<v.back()<<endl;
     v.pop_back();
     cout<<"Now the vector becomes: ";
     for(auto it = v.begin(); it!=v.end(); it++){
         cout<<*it<<" ";
     }
     cout<<endl;
     
     cout<<"Inserting 5 as the first element: ";
     auto it= v.begin();
     v.insert(it,5);
     cout<<"Now the vector becomes: ";
     for(auto it = v.begin(); it!=v.end(); it++){
         cout<<*it<<" ";
     }
         if (v.empty())
    cout << "\nvector is empty";
  else
    cout << "\nvector is not empty" << endl;

  v.clear();
  cout << "Size of the vector after clearing the vector: " << v.size();
     }
    
